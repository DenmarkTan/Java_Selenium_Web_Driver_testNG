package runnable_interface;

public class Main {
	public static void main(String[] args) {
		Thread thread1 = new Thread(new RunnableExample());
		thread1.start();
		
		Thread thread2 = new Thread(new RunnableExample());
		@Override
		public void run() {
			int i = 0;
			while(i <=100 ) {
				System.out.println();
			}
		}
		thread2.start();
	}

}
