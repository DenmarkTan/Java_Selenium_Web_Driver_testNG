package runnable_interface;

public class Main {
	public static void main(String[] args) {
		Thread thread1 = new Thread(new RunnableExample());
		thread1.start();
		
		Thread thread2 = new Thread(();
		@Override
		public void run() {
			int i = 0;
			while(i <=100 ) {
				System.out.println(i + " " + Thread.currentThread.getname());
				i++;
			}
		}
	});
	thread2.start();	
}

